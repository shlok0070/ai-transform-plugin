<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="com.example.ai-transform-plugin-tests" name="ai-transform-plugin" plugins-version="2"> 
  <plugin-info> 
    <description>This is the com.example:ai-transform-plugin plugin for Atlassian JIRA.</description>  
    <version>1.0.0-SNAPSHOT</version>  
    <vendor name="Example Company" url="http://www.example.com/"/> 
  </plugin-info>  
  <!-- from our base plugin -->  
  <component-import key="myComponent" interface="com.example.aitransform.api.MyPluginComponent"/>  
  <!-- from the product container -->  
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>  
  <component key="my-component-wired-test" class="it.com.example.aitransform.MyComponentWiredTest" public="true">
    <interface>it.com.example.aitransform.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
